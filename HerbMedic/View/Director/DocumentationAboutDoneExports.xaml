<Window x:Class="HerbMedic.View.DocumentationAboutDoneExports"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HerbMedic.View"
        mc:Ignorable="d" WindowStyle="None" WindowStartupLocation="CenterScreen"
        Title="DocumentationAboutDoneExports" Height="210" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="/images/templates/ExportEquipment.png"/>
    </Window.Background>
    <Grid>
        <Button HorizontalAlignment="Left" Height="30" Margin="25,0,0,0" VerticalAlignment="Top" Width="39" Background="#FF209E67" BorderBrush="#FF00182D" Click="ButtonGoBack">
            <Image Source="/images/details/backarrowBlue.png" Width="26"></Image>
        </Button>
        <DataGrid SelectionMode="Single" x:Name="dg_exports" HorizontalAlignment="Left" Height="157" Margin="25,35,0,0" VerticalAlignment="Top" Width="257"/>
        <Label Content="Previously done exports:" HorizontalAlignment="Left" Margin="69,0,0,0" VerticalAlignment="Top" Foreground="#FF00182D" FontSize="17px"/>
    </Grid>
</Window>
